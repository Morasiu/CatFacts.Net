<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>CatFacts.Net</AssemblyName>
    <RootNamespace>CatFacts.Net</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.0</Version>
    <Description>C# Client library for consuming Cat Facts API</Description>
    <PackageProjectUrl>https://github.com/Morasiu/CatFacts.Net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Morasiu/CatFacts.Net</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
    <PackageReference Include="RestSharp" Version="106.11.4" />
  </ItemGroup>

</Project>
